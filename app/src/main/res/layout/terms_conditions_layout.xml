<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:background="@color/dark_tone_sportify">

    <Button
            android:text="@string/continue_label"
            android:layout_width="0dp"
            android:layout_height="60dp" android:id="@+id/button110"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="32dp" app:cornerRadius="20dp"
            android:onClick="continueEvent"/>
    <TextView
            android:text="Privacy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView29"
            android:textColor="@color/white" android:textSize="18sp"
            app:layout_constraintTop_toTopOf="@+id/imageView20"
            app:layout_constraintBottom_toBottomOf="@+id/imageView20"
            app:layout_constraintStart_toEndOf="@+id/imageView20" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="55dp"
            android:layout_height="55dp" app:srcCompat="@drawable/ic_search" android:id="@+id/imageView20"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" app:layout_constraintTop_toBottomOf="@+id/textView30"
            android:layout_marginTop="32dp"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="120dp"
            android:id="@+id/textViewTitle3" android:textColor="@color/white"
            android:textStyle="bold" android:textAlignment="textStart" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:background="@color/dark_tone_sportify" android:textSize="30sp"
            android:paddingTop="40dp" android:text="Terms"
            android:paddingEnd="16dp" android:paddingStart="70dp"/>
    <ImageButton
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/ic_arrow" android:id="@+id/imageButton12"
            android:tint="@color/white" android:backgroundTint="@color/dark_tone_sportify" android:rotation="180"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" android:layout_marginStart="16dp" android:scaleType="fitCenter"
            android:onClick="onBackPressed"/>
    <TextView
            android:text="Please read our terms and conditions"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView27"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/imageButton12"
            android:layout_marginTop="8dp" android:layout_marginStart="70dp" android:textColor="@color/grey_sportify"
            android:textStyle="italic"/>
    <TextView
            android:text="Who May Use the Services?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView28"
            android:textColor="@color/white" android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/imageView19" app:layout_constraintTop_toTopOf="@+id/imageView19"
            android:layout_marginStart="16dp" app:layout_constraintBottom_toBottomOf="@+id/imageView19"/>
    <ImageView
            android:layout_width="55dp"
            android:layout_height="55dp" app:srcCompat="@drawable/ic_user" android:id="@+id/imageView19"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textViewTitle3"
            android:layout_marginTop="32dp" android:layout_marginStart="32dp"/>
    <TextView
            android:text="@string/lorem"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/textView30"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/imageView19"
            android:layout_marginTop="32dp" android:layout_marginStart="16dp" android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp" android:textAlignment="center"/>
    <TextView
            android:text="@string/lorem"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/textView31"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp" android:textAlignment="center"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toBottomOf="@+id/imageView20"
            android:layout_marginTop="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>